SELECT VSIT_COMP_NAME As "vsitCompName"
FROM TA_VSIT_RSVTN_M M, TA_VSIT_RSVTN_D D
WHERE M.VSIT_RSVTN_M_ID = D.VSIT_RSVTN_M_ID
AND TO_NUMBER(VSIT_RSVTN_FROM_DATE) BETWEEN TO_NUMBER(TO_CHAR(SYSDATE-90, 'yyyyMMdd')) AND TO_NUMBER(TO_CHAR(SYSDATE, 'yyyyMMdd'))
GROUP BY VSIT_COMP_NAME
ORDER BY VSIT_COMP_NAME